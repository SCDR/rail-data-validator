{
    "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
    "productName": "data-validation-tauti",
    "version": "0.1.0",
    "identifier": "com.ysc.datavalidationtauti",
    "build": {
        "frontendDist": "../dist",
        "devUrl": "http://localhost:8000",
        "beforeDevCommand": "pnpm dev",
        "beforeBuildCommand": "pnpm build"
    },
    "app": {
        "windows": [
            {
                "title": "数据验证",
                "width": 800,
                "height": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "android": {
            
        }
    }
}